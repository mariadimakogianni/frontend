import { createApp } from 'vue'
import App from './App.vue'
import router from './router'
import store from './store'
import vuetify from './plugins/vuetify'
import { loadFonts } from './plugins/webfontloader'
import { library } from '@fortawesome/fontawesome-svg-core'
import { FontAwesomeIcon } from '@fortawesome/vue-fontawesome'
import { fas } from '@fortawesome/free-solid-svg-icons' // Import all solid icons
import { far } from '@fortawesome/free-regular-svg-icons' // Import all regular icons

// Add all solid icons to the library
library.add(fas)

// Add all regular icons to the library
library.add(far)

loadFonts()

createApp(App)
  .use(router)
  .use(store)
  .use(vuetify)
  .component('font-awesome-icon', FontAwesomeIcon)
  .mount('#app')

  console.log("Start");

  store.state.Events=[
    {id:"testevent",type:"Today",eventName:"Teeth",description:"Brush Your Teeth.",timestamp:"2023-10-25T17:51:21+0000",frequency:"Daily"},
    ];
console.log(store.state.Events);

console.log(JSON.stringify(store.state.Events));

axios.get("localhost:3000/getEvents")
  .then(response => {
    var res = JSON.parse(atob(response.data));
    if (res.error === 1) {
      alert('There was an error creating the database! Please try again!');
    } else {
	    console.log(res);
    }
  })
  .catch(error => {
  });

